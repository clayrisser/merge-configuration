    "merge",
    "conf",
    "configuration"
  ],
  "homepage": "https://github.com/codejamninja/merge-configuration",
  "bugs": {
    "url": "https://github.com/codejamninja/merge-configuration/issues",
    "email": "jam@codejam.ninja"
