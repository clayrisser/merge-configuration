    "start": "babel-node src",
    "test": "npm run lint && jest"
  },
  "dependencies": {
    "babel-jest": "^23.6.0",
    "lodash.clone": "^4.5.0",
    "lodash.mergewith": "^4.6.1",
    "lodash.uniq": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-everything": "^0.1.2",
    "eslint": "^5.5.0",
    "eslint-config-jam": "^0.1.2",
    "eslint-plugin-jest": "^21.22.0",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  }
